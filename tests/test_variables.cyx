//Valid declarations and assignments with different types

program() {
    int age;
    float height;
    char initial;
    boolean active;

    age = 25;
    height = 1.72;
    initial = 'G';
    active = 1 != 1;

    printf("Age: %d\n", age);
    printf("Height: %.2f\n", height);
    printf("Initial: %c\n", initial);
    printf("Active: %d\n", active);
}
